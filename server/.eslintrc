{
  "extends": "eslint-config-egg/typescript",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "array-callback-return": 0,
    "arrow-parens": 0,
    "semi": 0,
    "array-bracket-spacing": 0,
    "@typescript-eslint/semi": 0,
    "@typescript-eslint/no-unused-vars": 0,
    "@typescript-eslint/no-var-requires": 0
  }
}
